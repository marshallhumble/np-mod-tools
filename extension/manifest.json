{
  "manifest_version": 2,
  "name": "NP Mod Helper",
  "description": "",
  "version": "1.2",
  "permissions": [
    "https://*.reddit.com/",
    "http://*.reddit.com/",
    "cookies",
    "identity",
    "storage"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://*.reddit.com/*",
        "http://*.reddit.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "128": "icon_128.png"
  }
}
